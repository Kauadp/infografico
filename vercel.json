{
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/infograficos/kanban_geral.html"
    },
    {
      "source": "/infografico",
      "destination": "/infograficos/infografico_jun_2025.html"
    },
    {
      "source": "/css/(.*)",
      "destination": "/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/js/$1"
    },
    {
      "source": "/",
      "destination": "/infograficos/index.html"
    }
  ]
}